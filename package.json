{
  "name": "osuindiajs",
  "version": "1.0.0",
  "description": "Built Exclusively for OsuIndia",
  "main": "src/index.ts",
  "scripts": {
    "type-check": "tsc --noEmit",
    "type-check:watch": "npm run type-check -- --watch",
    "build:dev": "npm run build:types && npm run build:js:dev",
    "build:prod": "npm run build:types && npm run build:js:prod",
    "build:types": "tsc --emitDeclarationOnly",
    "lint": "eslint ./src",
    "build:js:dev": "webpack --config webpack.config.dev.js",
    "build:js:prod": "webpack --config webpack.config.prod.js",
    "develop": "npm run lint && npm run build:dev && npm run start:bundle:dev",
    "start": "npm run develop",
    "dist": "npm run lint && npm run build:prod && npm run start:bundle:prod",
    "start:bundle:dev": "node ./build/app.bundle.js",
    "start:bundle:prod": "node ./dist/app.bundle.js"
  },
  "keywords": [
    "Osu",
    "Discord-Bot"
  ],
  "author": "XavierSchiller",
  "license": "ISC",
  "dependencies": {
    "eris": "^0.9.0",
    "nedb": "^1.8.0",
    "node-osu": "^1.3.2",
    "sqlite3": "^4.0.4"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.4",
    "@babel/plugin-proposal-class-properties": "^7.4.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.4.4",
    "@babel/preset-env": "^7.4.4",
    "@babel/preset-typescript": "^7.3.3",
    "@types/node": "^10.0.0",
    "babel-loader": "^8.0.5",
    "babel-preset-env": "^1.7.0",
    "eslint": "^5.16.0",
    "eslint-config-google": "^0.12.0",
    "fork-ts-checker-webpack-plugin": "^1.3.0",
    "typescript": "^3.4.5",
    "webpack": "^4.30.0",
    "webpack-cli": "^3.3.2",
    "webpack-node-externals": "^1.7.2"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/XavierSchiller/OsuIndiaBotJS.git"
  },
  "bugs": {
    "url": "https://github.com/XavierSchiller/OsuIndiaBotJS/issues"
  },
  "homepage": "https://github.com/XavierSchiller/OsuIndiaBotJS#readme"
}
